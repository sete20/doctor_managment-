{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "astrotomic/laravel-translatable": "^11.9",
        "barryvdh/laravel-dompdf": "^0.9.0",
        "cohensive/embed": "dev-master",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "intervention/image": "^2.5",
        "jackiedo/dotenv-editor": "^1.2",
        "janiskelemen/laravel-setting": "^1.2",
        "laracasts/flash": "^3.2",
        "laravel/framework": "^8.12",
        "laravel/horizon": "^5.7",
        "laravel/legacy-factories": "^1.1",
        "laravel/passport": "^10.1",
        "laravel/socialite": "^5.2",
        "laravel/tinker": "^2.5",
        "laravel/ui": "^3.4",
        "laravelcollective/html": "^6.2",
        "league/flysystem-aws-s3-v3": "~1.0",
        "mcamara/laravel-localization": "^1.6",
        "mostafasewidan/sewidan-field": "dev-master",
        "nwidart/laravel-modules": "^8.2",
        "pbmedia/laravel-ffmpeg": "^7.5",
        "pion/laravel-chunk-upload": "^1.5",
        "predis/predis": "^1.1",
        "rachidlaasri/laravel-installer": "^4.1",
        "spatie/laravel-activitylog": "^3.17",
        "spatie/laravel-medialibrary": "^8.0.0",
        "spatie/laravel-permission": "^4.2",
        "spatie/laravel-translatable": "^4.6",
        "spatie/valuestore": "^1.2",
        "swooletw/laravel-swoole": "^2.6",
        "unisharp/laravel-filemanager": "^2.2",
        "vimeo/laravel": "^5.7"
    },
    "require-dev": {
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "Modules/",
            "Helper\\": "Modules/App/MyHelper/",
            "Traits\\": "Modules/App/Traits/",
            "Api\\": "app/Http/Controllers/Api",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
